{% extends 'base.html.twig' %}

{% block title %}Programmes immobiliers neufs{% endblock %}

{% block body %}

<div class="container">
    {% for program in programs %}
    <div style="margin-bottom: 10px;">
        <h1>{{ program.title }}</h1>
        <p>{{ program.name }}</p>
        <p>{{ program.description }}</p>
    </div>
    {% endfor %}
</div>
{% endblock %}
