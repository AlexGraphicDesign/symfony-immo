{% extends 'base.html.twig' %}

{% block title %}{{ program.title }}{% endblock %}

{% block body %}

<section class="py-2">
    <div class="container">
        <h1>{{ program.title }}</h1>
        <img src="{{ program.featuredImage }}" alt="{{ program.title }}">
        <p>{{ program.content }}</p>
        <p>{{ program.address }}</p>
    </div>
</section>
{% endblock %}
